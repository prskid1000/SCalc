<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_green_light"
    android:visibility="visible">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button4"
        android:layout_alignParentStart="true"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:text="@string/cos"
        android:visibility="visible" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button"
        android:layout_toEndOf="@+id/button"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:text="@string/acos"
        android:visibility="visible" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button14"
        android:layout_alignParentEnd="true"
        android:background="@android:color/holo_purple"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:text="@string/mul"
        android:visibility="visible" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button5"
        android:background="@android:color/holo_purple"
        android:paddingLeft="16dp"
        android:text="@string/tan"
        android:visibility="visible" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button3"
        android:layout_toStartOf="@+id/button3"
        android:background="@android:color/holo_purple"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:text="@string/div"
        android:visibility="visible" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button2"
        android:background="@android:color/holo_purple"
        android:text="@string/atan"
        android:visibility="visible" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/button3"
        android:background="@android:color/holo_purple"
        android:backgroundTint="@android:color/holo_purple"
        android:text="@string/log"
        android:visibility="visible" />

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button3"
        android:layout_toEndOf="@+id/button"
        android:background="@android:color/holo_purple"
        android:backgroundTint="@android:color/holo_purple"
        android:text="@string/exp"
        android:visibility="visible" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button14"
        android:layout_toStartOf="@+id/button3"
        android:background="@android:color/holo_purple"
        android:text="@string/mod"
        android:visibility="visible" />

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button15"
        android:layout_alignParentEnd="true"
        android:background="@android:color/holo_purple"
        android:text="@string/fact"
        android:visibility="visible" />

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button3"
        android:background="@android:color/holo_purple"
        android:text="@string/ncr"
        android:visibility="visible" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/holo_purple"
        android:text="@string/npr"
        android:visibility="visible" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button3"
        android:background="@android:color/holo_purple"
        android:text="@string/square"
        android:visibility="visible" />

    <Button
        android:id="@+id/button18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@android:color/holo_purple"
        android:text="@string/s_root"
        android:visibility="visible" />

    <Button
        android:id="@+id/button19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button3"
        android:layout_toEndOf="@+id/button"
        android:background="@android:color/holo_purple"
        android:text="@string/cube"
        android:visibility="visible" />

    <Button
        android:id="@+id/button20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button"
        android:background="@android:color/holo_purple"
        android:text="@string/c_root"
        android:visibility="visible" />

    <Button
        android:id="@+id/button25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button"
        android:layout_toEndOf="@+id/button2"
        android:background="@android:color/holo_purple"
        android:text="@string/sin"
        android:visibility="visible" />

    <Button
        android:id="@+id/button26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button"
        android:layout_toEndOf="@+id/button2"
        android:background="@android:color/holo_purple"
        android:text="@string/asin"
        android:visibility="visible" />

    <Button
        android:id="@+id/button27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/button4"
        android:layout_alignTop="@+id/button"
        android:background="@android:color/holo_purple"
        android:text="@string/add"
        android:visibility="visible" />

    <Button
        android:id="@+id/button28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button"
        android:layout_alignStart="@+id/button4"
        android:background="@android:color/holo_purple"
        android:text="@string/sub"
        android:visibility="visible" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_above="@+id/button3"
        android:layout_alignParentStart="true"
        android:background="@android:color/background_light"
        android:backgroundTint="@android:color/background_light"
        android:textAlignment="center"
        android:textAppearance="@android:style/TextAppearance"
        android:textSize="24sp"
        android:visibility="visible" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="60dp"
        android:background="@android:color/background_light"
        android:ems="10"
        android:hint="@string/second_operand"
        android:inputType="textPersonName|numberDecimal"
        android:visibility="visible" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@android:color/background_light"
        android:ems="10"
        android:hint="@string/start"
        android:inputType="textPersonName|numberSigned|numberDecimal"
        android:visibility="visible" />

</RelativeLayout>